{
  "name": "leaflet-area-select",
  "version": "1.0.5",
  "description": "Area select tool for leaflet",
  "main": "index.js",
  "directories": {
    "docs": "docs",
    "test": "test"
  },
  "scripts": {
    "prestart": "npm install",
    "test": "cross-env CI=1 browserify ./test/*.test.js | tape-puppet --no-sandbox",
    "start": "npm run watch & serve",
    "wstart": "serve -p 3001",
    "watch": "watchify -v -d -s L.Control.Select docs/js/app.js -o docs/js/bundle.js",
    "prebuild": "npm test",
    "build": "npm run build-js && npm run compress",
    "build-js": "cat src/Map.SelectArea.js > dist/Map.SelectArea.js",
    "compress": "uglifyjs ./dist/Map.SelectArea.js -m -c --comments drop_console=true,warnings=false > dist/Map.SelectArea.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w8r/leaflet-area-select.git"
  },
  "keywords": [
    "leaflet",
    "map",
    "area",
    "select",
    "bbox",
    "bounds"
  ],
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "leaflet": "global:L"
  },
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w8r/leaflet-area-select/issues"
  },
  "homepage": "https://github.com/w8r/leaflet-area-select#readme",
  "dependencies": {
    "leaflet": "^1.6.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.14",
    "cross-env": "^7.0.2",
    "serve": "^11.3.2",
    "smokestack": "^3.4.1",
    "tap-closer": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tap-status": "^1.0.1",
    "tape": "^4.6.3",
    "tape-puppet": "^0.3.0",
    "tape-run": "^10.0.0",
    "uglify-js": "^3.0.10",
    "watchify": "^3.9.0"
  }
}
