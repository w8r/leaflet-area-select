{
  "name": "leaflet-area-select",
  "version": "1.1.0",
  "description": "Area select tool for leaflet",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "types": "index.d.ts",
  "directories": {
    "docs": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "cross-env CI=1 browserify ./test/Map.SelectArea.test.js | tape-puppet --no-sandbox",
    "start": "vite -c docs/vite.config.mjs",
    "docs": "vite build -c ./docs/vite.config.mjs",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w8r/leaflet-area-select.git"
  },
  "keywords": [
    "leaflet",
    "map",
    "area",
    "select",
    "bbox",
    "bounds"
  ],
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "leaflet": "global:L"
  },
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w8r/leaflet-area-select/issues"
  },
  "homepage": "https://github.com/w8r/leaflet-area-select#readme",
  "peerDependencies": {
    "leaflet": "1.9.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.14",
    "cross-env": "^7.0.2",
    "gh-pages": "^6.1.1",
    "serve": "^14.1.2",
    "smokestack": "^3.4.1",
    "tap-closer": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tap-status": "^1.0.1",
    "tape": "^4.6.3",
    "tape-puppet": "^0.3.0",
    "tape-run": "^11.0.0",
    "uglify-js": "^3.0.10",
    "vite": "^5.3.1",
    "watchify": "^3.9.0"
  }
}
